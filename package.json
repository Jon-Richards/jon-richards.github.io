{
  "name": "jon-richards_github",
  "version": "1.0.0",
  "description": "Front-end for my github repository.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node ./server.ts",
    "build:dev": "npx esbuild ./src/index.ts --outfile=./dist/assets/index.js --bundle --sourcemap --watch",
    "build:prod": "npx esbuild ./src/index.ts --outfile=./dist/assets/index.js --bundle --minify",
    "lint": "npx eslint ./src/**/* || true",
    "typecheck": "npx tsc ./src/index.ts --noEmit",
    "validate": "nodemon --exec \"npx eslint ./src/**/* && tsc ./src/index.ts --noEmit\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jon-Richards/jon-richards.github.io.git"
  },
  "author": "Jon Richards",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Jon-Richards/jon-richards.github.io/issues"
  },
  "homepage": "https://github.com/Jon-Richards/jon-richards.github.io#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/node": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel": "^6.23.0",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "eslint-plugin-jsdoc": "^37.5.1",
    "express": "^4.17.2",
    "jest": "^27.4.5",
    "jest-puppeteer": "^6.0.3",
    "nodemon": "^2.0.15",
    "puppeteer": "^13.0.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
